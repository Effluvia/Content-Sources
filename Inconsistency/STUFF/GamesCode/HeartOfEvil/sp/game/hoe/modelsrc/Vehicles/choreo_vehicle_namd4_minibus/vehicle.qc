$modelname "Vehicles/choreo_vehicle_namd4_minibus.mdl"
$cdmaterials "models\Blackout"

$bodygroup studio
{
studio "vehicle.smd"
}

// .qc block generated by HLMV begins.

$surfaceprop "default"
$jointsurfaceprop "GhostAnim_Base"	 "default"


$hboxset "default"

$hbox 0 "GhostAnim_Base"	    -2.00   -2.00   -2.00     2.00    2.00    2.00

$hboxset "entryboxes"

$hbox 0 "GhostAnim_Base"	   -32.00    4.00  -36.00    32.00   68.00   36.00

// .qc block generated by HLMV ends.




$attachment "vehicle_driver_eyes" "GhostAnim_View" 0.00 0.00 0.00 rotate -90 -90 0
$attachment "vehicle_feet_passenger0" "GhostAnim_Base" 0.00 0.00 0.00 rotate -90 -90 0

// 1 sequences
$sequence idle "seq_idle" fps 10
$sequence enter1 "seq_enter1" fps 15 snap

$collisionmodel "vehicle.smd"
{
     // Mass in kilograms
     $mass 1000
}

$keyvalues
{
    "vehicle_entry"
    {
        // Entries are: Key = enter animation, Value = Hitbox group
        "enter1"	"0"
    }
}

