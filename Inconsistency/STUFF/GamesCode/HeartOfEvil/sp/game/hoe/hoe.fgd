// HOE: Added Useable
@BaseClass = Useable
[
	UseableString(string) : "Useable String" : "" : "HOE: String to display in hud_useable when the player is in position to use a useable entity. Can be set by the SetUseableString input."
	UseableAngles(angle) : "Useable Angles (Y Z X)" : "0 0 0" : "HOE: Together with 'Useable FOV' determines the direction the player must be facing to use this entity."
	UseableFOV(float) : "Useable FOV" : "-1" : "HOE: -1.0,1.0. Together with 'Useable Angles' determines the direction the player must be facing to use this entity."

	input SetUseableString(string) : "HOE: Sets the Useable String keyvalue."
	input SetUseableFOV(float) : "HOE: Sets the Useable FOV keyvalue."

	output OnUseableEnter(void) : "Fired when the player enters both proximity and orientation to use this entity."
	output OnUseableLeave(void) : "Fired when the player is no longer in proximity or orientation to use this entity."
]

@BaseClass = Angles
