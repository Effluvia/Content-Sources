#if     _MSC_VER > 1000
#pragma once
#endif

#ifndef _CLOCALE_
#define _CLOCALE_
#ifdef _STD_USING
 #undef _STD_USING
 #include <locale.h>
 #define _STD_USING
#else
 #include <locale.h>
#endif /* _STD_USING */
#endif /* _CLOCALE_ */

